{
  "name": "nodejs-express-mongoose-demo",
  "description": "A demo app illustrating the usage of express, mongoose, passportjs, jade and other modules in nodejs",
  "keywords": [
    "express",
    "mongoose",
    "mongodb",
    "passport",
    "demo"
  ],
  "version": "4.0.0",
  "private": false,
  "author": "Madhusudhan Srinivasa <madhums8@gmail.com> (http://madhums.github.com)",
